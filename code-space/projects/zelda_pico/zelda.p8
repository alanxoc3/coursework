pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
actors = {}

function _init()
 pl = add_actor(7,10.5,2)
 goomba = add_actor(14,14.5,5)
 goomba.direction = -.05
 music(0)
end

function _update()
 control_player(pl)
 foreach(actors,move_actor)
 foreach(actors,update_ground)
 goomba.dx += goomba.direction
end

function _draw()
 cls()
 map(0,0,0,0,16,16) 
 foreach(actors,draw_actor)
 print_debug()
end

function print_debug()
 print("x "..pl.x,0,10,7)
 print("y "..pl.y,0,20,7)
 
 if pl.ground then
  print("grd true",0,30,6)
 else
  print("grd false",0,30,6)
 end
end

function update_ground(a)
 local below = a.y + a.dim + .5
 if solid(a.x-a.dim,below) or
    solid(a.x+a.dim,below)
  then
   a.ground = true
  else
   a.ground = false
 end
 
 return a.ground
end

function control_player(pl)
 accel=.1
 jmp=.7
 -- left right up down
 if (btn(0))
  then
  pl.dx -= accel
  pl.flipped = false
 end
 
 if (btn(1))
  then
 	pl.dx += accel
 	pl.flipped = true
 end
 if (btnp(2) and pl.ground) pl.dy -= jmp
 if (btn(3)) pl.dy += accel
end

-- colliding with wall?
function will_collide(a,dx,dy)

 return solid_area(
  a.x + dx,
  a.y + dy,
  a.dim,
  a.dim)
end

function move_actor(a)
 a.dy -= a.gravity

 if not will_collide(a,a.dx,0)
 then
  a.x = a.x + a.dx
 else
  sfx(0)
  a.dx *= -a.bounce
  if (a == goomba)
   then
   a.direction = -a.direction
  end
 end
 
 if not will_collide(a,0,a.dy)
 then
  a.y = a.y + a.dy
 else
 	a.dy *= -a.bounce
 end
 
 a.dx *= a.inertia
end

function draw_actor(a)
 local sx = (a.x * 8) - 4
 local sy = (a.y * 8) - 4
 spr(a.img,sx,sy, 1, 1, a.flipped)
end

function add_actor(x, y, i)
 a={}
 a.x = x
 a.y = y
 a.dx = 0
 a.dy = 0
 a.img = i
 a.img_dim = 1
 a.dim = .4
 a.inertia = .6
 a.bounce = 0
 a.gravity = -2.4/30
 a.ground = false
 a.flipped = false
 add(actors,a)
 return a
end

-- collision stuff
function solid(x,y)
 local val=mget(x,y)
 return fget(val,0)
end

function solid_area(x,y,w,h)

 return
  solid(x-w,y-h) or
  solid(x+w,y-h) or
  solid(x-w,y+h) or
  solid(x+w,y+h)
end
